<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/main"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".ui.activity.PassedActivity">
<Button
    android:id="@+id/backButton"
    android:layout_width="183dp"
    android:layout_height="52dp"
    android:layout_marginStart="112dp"
    android:layout_marginTop="20dp"
    android:text="Back"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/displayCheckBox" />

    <TextView
        android:id="@+id/displayName"
        android:layout_width="250dp"
        android:layout_height="50dp"
        android:layout_marginStart="78dp"
        android:layout_marginTop="27dp"
        android:fontFamily="@font/arizonia"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/welcomeImageView" />
<ImageView
    android:id="@+id/welcomeImageView"
    android:layout_width="311dp"
    android:layout_height="259dp"
    android:layout_marginStart="48dp"
    android:layout_marginTop="40dp"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:srcCompat="@drawable/welcome" />
<TextView
    android:id="@+id/displayEmail"
    android:layout_width="250dp"
    android:layout_height="50dp"
    android:layout_marginStart="78dp"
    android:layout_marginTop="3dp"
    android:textStyle="bold"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/displayName" />
<TextView
    android:id="@+id/displayPassword"
    android:layout_width="250dp"
    android:layout_height="50dp"
    android:layout_marginStart="78dp"
    android:layout_marginTop="5dp"
    android:textStyle="bold"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/displayEmail" />
<TextView
    android:id="@+id/displayGender"
    android:layout_width="250dp"
    android:layout_height="50dp"
    android:layout_marginStart="78dp"
    android:textStyle="bold"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/displayPassword" />
<TextView
    android:id="@+id/displayCountry"
    android:layout_width="250dp"
    android:layout_height="50dp"
    android:layout_marginStart="79dp"
    android:layout_marginTop="5dp"
    android:textStyle="bold"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/displayGender" />
<TextView
    android:id="@+id/displayCheckBox"
    android:layout_width="250dp"
    android:layout_height="50dp"
    android:layout_marginStart="78dp"
    android:layout_marginTop="3dp"
    android:textStyle="bold"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/displayCountry" />
</androidx.constraintlayout.widget.ConstraintLayout>